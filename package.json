{
  "name": "linaria-styled",
  "version": "0.0.0",
  "private": true,
  "description": "Experimental Babel plugin to extract CSS statically",
  "main": "src/index.js",
  "files": [
    "src/"
  ],
  "repository": "git@github.com:satya164/linaria-styled.git",
  "author": "Satyajit Sahoo <satyajit.happy@gmail.com>",
  "license": "MIT",
  "keywords": [
    "babel-plugin",
    "babel",
    "module",
    "require",
    "import"
  ],
  "scripts": {
    "build": "babel src --out-dir lib --ignore '**/__tests__/**' --source-maps && flow-copy-source -i '**/__tests__/**' src lib",
    "example": "webpack-serve --config example/webpack.config.js",
    "lint": "eslint .",
    "flow": "flow",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "dedent": "^0.7.0",
    "eslint": "^5.4.0",
    "eslint-config-satya164": "^2.0.1",
    "flow-bin": "^0.79.1",
    "flow-copy-source": "^2.0.2",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.2",
    "prettier": "^1.14.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "source-map": "^0.7.3",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "stylis": "^3.5.3"
  },
  "resolutions": {
    "**/babel-core": "7.0.0-bridge.0"
  }
}
